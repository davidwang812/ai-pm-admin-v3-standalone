{
  "name": "ai-pm-admin-v3-standalone",
  "version": "3.0.0",
  "description": "AI Product Manager Admin V3 - Standalone Version",
  "private": true,
  "scripts": {
    "dev": "vercel dev",
    "build": "mkdir -p dist && cp -r *.html _* api dist/ 2>/dev/null || true",
    "start": "vercel",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint '_core/**/*.js' '_pages/**/*.js' '_components/**/*.js'",
    "lint:fix": "eslint '_core/**/*.js' '_pages/**/*.js' '_components/**/*.js' --fix"
  },
  "dependencies": {
    "jose": "^5.2.0"
  },
  "devDependencies": {
    "vercel": "^44.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.7.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/dom": "^9.3.0",
    "eslint": "^8.51.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}